{
  "extends": "plugin:vue/recommended",
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-plusplus": 0,
    "prefer-destructuring": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-unused-expressions": 0,
    "import/prefer-default-export": 0
  },
  "globals": {
    "M": true,
    "flvjs": true
  }
}
